<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <!-- 配置别名 -->
    <typeAliases>
        <!-- 批量加载别名，制定批量定义别名的类包即可，别名为类民 -->
        <package name="cn.liupeng.model"/>
    </typeAliases>

    <!-- 批量加载映射文件 -->
    <mappers>
        <package name="cn.liupeng.mapper"/>
    </mappers>
</configuration>